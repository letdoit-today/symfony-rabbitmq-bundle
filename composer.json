{
    "name": "letdoittoday/rabbitmq-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "php-amqplib/php-amqplib": "^2.11",
        "symfony/dotenv": "5.0.*"
    },
    "require-dev": {
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DIT\\RabbitMQBundle\\": ""
        },
        "exclude-from-classmap": [
            "Resources/",
            "Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}
